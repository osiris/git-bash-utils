alias glg="git log --topo-order --date=iso --pretty --format=\"%h %ad %ae %s\" | sed s/:[0-9]*\ -[0-9]*\ /\ /g | sed s/@[a-z.\(\)]*/\\\t/g"
alias glh="glg | head"
